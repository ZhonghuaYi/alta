env = Environment()
env.Append(CPPPATH = ['../../../external/build/include', '../../'])
env.Append(LIBPATH = ['../../build'])

conf = Configure(env)
if conf.CheckLib('flann'):

	sources = ['data.cpp']
	libs = ['flann', 'core']
	env.SharedLibrary('../../build/data_interpolant', sources, LIBS=libs)

#end
env = conf.Finish()
